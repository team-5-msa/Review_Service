{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/public/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/public/:path*",
      "destination": "/public/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/api/[...all]"
    }
  ]
}
